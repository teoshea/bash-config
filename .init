GIT_HOME_CONFIG=linux-1
SSH_ASKPASS=/usr/bin/ssh-askpass

## PATH
PATH=$home/bin:$PATH

__source_files()
{
	for f in `find -L $1 -maxdepth 1 -type f \( -name "*$2*" -and ! -iname ".*" -and ! -iname "~*" \) | sort`
	do
		. $f
	done
}

INIT_FOLDER="$HOME/.init.d/$GIT_HOME_CONFIG"

if [ -d $INIT_FOLDER ] ; then
  __source_files $INIT_FOLDER ""
fi

cd $HOME
